child_pid = fork();
        if (child_pid == -1)

        {
                perror("Error:");
                return (1);
        }
        sususay_pid = getpid();
        printf("sususay pid is %u\n", sususay_pid);
        if (child_pid == 0)
        {
                printf("(%u) Incorrect!\n", sususay_pid);
        }
        else
        {
                printf("(%u) %u, This is the parent\n", sususay_pid, child_pid);
        }
        return (0);
